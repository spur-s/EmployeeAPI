        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="129dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
